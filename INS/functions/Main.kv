#:import get_color_from_hex kivy.utils.get_color_from_hex
# <MoveWindow>:

<ISD>
    BoxLayout:
        orientation: "vertical"
        size: root.width,root.height

        ActionBar:
            orientation: "horizontal"
            pos_hint: {'top':1}
            height: 50
            background_color: [0.95,0.95,0.95,1]

            ActionView:
                use_separator: True

                ActionPrevious:
                    app_icon:'Image/title_bar/logo.png'
                    title: 'Images Screening Detection'
                    color:'#ffffff'
                    with_previous: False

                ActionOverflow:

                MyActionButton:
                    icon: 'Image/title_bar/reless_hover.png' if self.hovered else 'Image/title_bar/reless.png'
                    # if root.hovered else 'Image/title_bar/reless_hover.png'
                    width: 40 if self.hovered else 40
                    on_press: app.Minus_app_button()
                    border: 10,10,10,10
                MyActionButton:
                    icon: 'Image/title_bar/exit_hover.png' if self.hovered else 'Image/title_bar/exit.png'
                    # if root.hovered else 'Image/title_bar/exit_hover.png'
                    width: 40 if self.hovered else 40
                    on_press: app.close_app_button()
                    border: 10,10,10,10

        BoxLayout:

            canvas.before:
                Color:
                    rgba: (1, 1, 1, 1)
                Rectangle:
                    source:'Image/BG.jpg'
                    size: root.width, root.height-50
                    pos: self.pos
            
            MDNavigationRail:
                id: rail
                md_bg_color: get_color_from_hex("#141414")
                color_normal: get_color_from_hex("#FFFFFF")
                color_active: get_color_from_hex("#223C5B")
                use_hover_behavior: True
                hover_bg: 0, 0, 0, 1

                MDNavigationRailItem:
                    id:screening
                    icon: "image-search-outline"
                    # text: "Screening"
                    font_size:24
                    pos_hint: {"center_x": .5,"center_y": .5}
                    size_hint: 1,1
                    on_release:
                        screen_manager.transition.direction = 'right'
                        screen_manager.current = "screening"

                MDNavigationRailItem:
                    id:training
                    icon: "head-lightbulb-outline"
                    # text: "Training"
                    font_size:24
                    on_release:
                        screen_manager.current = "trainng"

                MDNavigationRailItem:
                    id:tutorial
                    icon: "book-play-outline"
                    # text: "Tutorial"
                    font_size:24
                    on_release:
                        screen_manager.current = "tutorial"

                # MDNavigationRailItem:
                #     id:setting
                #     icon: "cogs"
                #     text: "Setting"
                #     font_size:24
                #     on_release:
                #         screen_manager.current = "setting"

